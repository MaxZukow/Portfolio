<% @title = t(:title_elephantbleu) %>

        
<!-- PAGEBODY -->
<section id="page-body">

    <div class="wrapper-small project-space">
    
        <h4 class="align-center"><strong><%= t(:elephantbleu_title) %></strong></h4>
        <h6 class="alttitle align-center"><%= t(:elephantbleu_note) %></h6>
        
        <div class="column-section clearfix">
            <div class="column one-half">
                <p><%= t(:elephantbleu_text) %></p>
            </div>
            <div class="column one-half last-col">
                <p>
                    <strong><%= t(:year) %></strong> 2014
                    <br />
                    <strong><%= t(:client) %></strong> <%= t(:elephantbleu) %> 
                    <br />
                    <strong><%= t(:agency) %></strong> <a style="color: black" href="http://www.advisa.fr">Advisa</a> 
                    <br />
                    <br />
                    <!-- <a href="http://www.elephantbleu.fr/trouver-votre-centre-de-lavage-el%C3%A9phant-bleu.html"><%= t(:view_the_project) %></a> -->
                </p>
            </div>
        </div>
                    
    </div>
            
    <div class="wrapper">
        <div class="project-img">
            <%= image_tag 'projects/elephantbleu-1.jpg', :alt => '' %>
        </div>
        <div class="project-img">
            <%= image_tag 'projects/elephantbleu-3.jpg', :alt => '' %>
        </div>
        <div class="project-img">
            <%= image_tag 'projects/elephantbleu-2.jpg', :alt => '' %>
        </div> 
        <div class="project-img">
            <%= image_tag 'projects/elephantbleu-4.jpg', :alt => '' %>
        </div> 
    </div> <!-- END .wrapper -->
            
    <!-- Single Pagination -->
    <div class="wrapper">
        <ul class="single-pagination">
            <li class="prev">
                <% link_to '/projects/stabilo.html' do %>
                    <%= image_tag 'work-1.jpg', :alt => t(:prev) %>
                    <span><%= t(:previous_project) %></span>
                <% end %>
            </li>
            <li class="backtoworks"><a href="/"><%= link_to t(:back_to_works), '/' %></a></li>
            <li class="next">
                <% link_to '/projects/kolb.html' do %>
                    <%= image_tag 'work-4.jpg', :alt => t(:next) %>
                    <span><%= t(:next_project) %></span>
                <% end %>
            </li>
        </ul>
    </div>
    <!-- Single Pagination -->       
    
    </section>
<!-- PAGEBODY -->